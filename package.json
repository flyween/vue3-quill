{
  "name": "vue3-quill",
  "version": "0.3.1",
  "private": false,
  "main": "src/index.js",
  "typings": "src/index.d.ts",
  "unpkg": "lib/vue3-quill.umd.min.js",
  "jsdelivr": "lib/vue3-quill.umd.min.js",
  "keywords": [
    "vue3",
    "quill",
    "editor",
    "RichText"
  ],
  "description": "Quill editor for vue3",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "lib": "vue-cli-service build --target lib --name vue3-quill --dest lib src/index.js"
  },
  "files": [
    "lib",
    "src"
  ],
  "dependencies": {
    "core-js": "^3.6.5",
    "quill": "^1.3.7",
    "vue": "^3.0.11"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.5.12",
    "@vue/cli-plugin-eslint": "^4.5.12",
    "@vue/cli-service": "^4.5.12",
    "@vue/compiler-sfc": "^3.0.11",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^7.0.0-0",
    "vue-loader": "^16.2.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/flyween/vue3-quill.git"
  },
  "homepage": "https://flyween.github.io/vue3-quill",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/flyween/vue3-quill/issues"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
